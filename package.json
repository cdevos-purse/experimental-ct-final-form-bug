{
  "name": "@upstreampay/partner-ui",
  "private": false,
  "version": "0.13.0",
  "description": "Purse partner ui webcomponent",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist/*",
    "node_modules/*",
    "custom-elements.json"
  ],
  "type": "module",
  "scripts": {
    "analyze": "cem analyze --litelement --globs \"src/**/*.ts\"",
    "analyze:watch": "cem analyze --litelement --globs \"src/**/*.ts\" --watch",
    "build": "tsc",
    "build:serve": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist/ src/test/",
    "docs": "npm run docs:clean && npm run build && npm run analyze && npm run docs:build && npm run docs:assets && npm run docs:gen",
    "docs:assets": "cp node_modules/prismjs/themes/prism-okaidia.css docs/",
    "docs:build": "rollup -c --file docs/index.bundled.js",
    "docs:clean": "rimraf docs",
    "docs:gen": "eleventy --config=.eleventy.cjs",
    "docs:gen:watch": "eleventy --config=.eleventy.cjs --watch",
    "docs:serve": "wds --root-dir=docs --node-resolve --watch",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "new:plugin": "cookiecutter -o src/plugins/partners _templates/plugin",
    "serve": "wds --watch --port 8000",
    "serve:prod": "MODE=prod npm run serve",
    "test": "npm run test:spec && npm run test:e2e",
    "test:dev": "dotenv -- concurrently \"npm run test:spec:dev\" \"npm run test:e2e:dev\"",
    "test:e2e": "npx playwright test",
    "test:e2e:dev-headless": "dotenv -- npx playwright test ",
    "test:e2e:dev": "dotenv -- npx playwright test --ui",
    "test:spec": "vitest",
    "test:forCoverage": "vitest --coverage",
    "test:spec:dev": "vitest -w",
    "test:utils": "dotenv -- node --experimental-specifier-resolution=node --loader ts-node/esm server/testSuiteUtils.mjs --watch",
    "test-ct": "playwright test -c playwright-ct.config.ts"
  },
  "keywords": [
    "web-components",
    "payment",
    "purse",
    "parnter-ui",
    "lit"
  ],
  "author": "Purse",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@babel/preset-env": "^7.25.4",
    "@custom-elements-manifest/analyzer": "^0.6.3",
    "@playwright/test": "1.48.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@sand4rt/experimental-ct-web": "^1.48.2",
    "@types/mocha": "^10.0.7",
    "@types/node": "^22.5.3",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "@vitest/coverage-v8": "^2.1.5",
    "@web/dev-server": "^0.4.6",
    "@web/dev-server-esbuild": "^1.0.2",
    "@web/dev-server-legacy": "^1.0.0",
    "@web/test-runner": "^0.19.0",
    "@web/test-runner-playwright": "^0.11.0",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.15.0",
    "express": "^4.19.2",
    "isomorphic-fetch": "^3.0.0",
    "lodash.partition": "^4.6.0",
    "lit-analyzer": "^2.0.3",
    "prettier": "^2.6.2",
    "playwright": "1.48.2",

    "rimraf": "^3.0.2",
    "rollup": "^2.73.0",
    "rollup-plugin-summary": "^1.4.3",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3",
    "vitest": "^2.1.5"
  },
  "overrides": {
  },
  "customElements": "custom-elements.json",
  "dependencies": {
    "@types/googlepay": "^0.7.6",
    "final-form": "^4.20.10",
    "lit": "^3.2.0"

  }
}
